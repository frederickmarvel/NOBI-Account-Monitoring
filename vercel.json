{
  "version": 2,
  "builds": [
    {
      "src": "backend/backend.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/backend.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "env": {
    "ETHERSCAN_API_KEY": "@etherscan_api_key"
  }
}
